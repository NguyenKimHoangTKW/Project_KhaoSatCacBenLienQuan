    <script src="~/Style/assets/js/jquery.timepicker.min.js"></script>
    <script src="~/Style/assets/js/scrollax.min.js"></script>
    <script src="~/Style/assets/js/main.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <script>
        function goBack() {
            window.history.back();
        }
    </script>
    <script src="~/Scripts/login-with-google.js"></script>
    @Scripts.Render("~/bundles/signalr")
    <script src="~/signalr/hubs"></script>
    @Scripts.Render("~/bundles/firebase")
    @RenderSection("scripts", required: false)
</body>
</html>
