@model List<string>

<div class="file-wrapper m-t-20">
    @foreach (var folder in Model)
    {
        <div class="file">
            <div class="media align-items-center">
                <div class="m-r-15 font-size-30">
                    <i class="anticon anticon-folder text-warning"></i>
                </div>
                <div>
                    <h6 class="mb-0">@folder</h6>
                </div>
            </div>
        </div>
    }
</div>
